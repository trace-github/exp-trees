    "mkdirp": "^3.0.1",
    "object-hash": "^3.0.0",
    "parquet-wasm": "0.6.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/object-hash": "^3.0.6",
    "esbuild-plugin-wasm": "^1.1.0"
  }
